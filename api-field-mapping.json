{
  "metadata": {
    "version": "1.1.0",
    "lastUpdated": "2025-09-17",
    "description": "API field mapping configuration for crypto tracker data standardization"
  },
  "typeDefinitions": {
    "currencyAmount": {
      "description": "A monetary amount with explicit currency specification",
      "structure": {
        "amount": {
          "type": "number",
          "description": "The numeric amount"
        },
        "currency": {
          "type": "string",
          "description": "The currency code (USD, EUR, JPY, etc.)"
        }
      },
      "example": {
        "amount": 45000.50,
        "currency": "USD"
      }
    },
    "tokenAmount": {
      "description": "A token/asset quantity with token specification",
      "structure": {
        "amount": {
          "type": "number",
          "description": "The numeric quantity"
        },
        "currency": {
          "type": "string",
          "description": "The token/asset symbol (BTC, ETH, AAPL, etc.)"
        }
      },
      "example": {
        "amount": 1500000,
        "currency": "BTC"
      }
    }
  },
  "apis": {
    "figureMarkets": {
      "name": "Figure Markets API",
      "description": "Crypto trading data from Figure Markets",
      "baseUrl": "https://api.figuremarkets.com"
    },
    "yahooFinance": {
      "name": "Yahoo Finance API",
      "description": "Stock and some crypto data",
      "baseUrl": "https://query1.finance.yahoo.com"
    },
    "coinGecko": {
      "name": "CoinGecko API",
      "description": "Comprehensive crypto market data",
      "baseUrl": "https://api.coingecko.com/api/v3"
    },
    "alphaVantage": {
      "name": "Alpha Vantage API",
      "description": "Stock and crypto data with time series",
      "baseUrl": "https://www.alphavantage.co/query"
    },
    "freeCurrencyApi": {
      "name": "FreeCurrencyAPI",
      "description": "Currency exchange rates",
      "baseUrl": "https://api.freecurrencyapi.com/v1"
    },
    "exchangeRateHost": {
      "name": "ExchangeRate.host",
      "description": "Real-time currency exchange rates",
      "baseUrl": "https://api.exchangerate.host"
    },
    "fixerIo": {
      "name": "Fixer.io",
      "description": "Professional currency exchange rates",
      "baseUrl": "https://api.fixer.io/v1"
    }
  },
